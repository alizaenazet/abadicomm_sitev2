    "astro-og-canvas": "^0.7.2",
    "astro-robots-txt": "^1.0.0",
    "astro-seo": "^0.8.4",
    "svelte": "^5.45.6",
    "tailwindcss": "^4.1.17",
    "typescript": "^5.9.3"
  },
  "devDependencies": {
    "@lucide/svelte": "^0.556.0",
